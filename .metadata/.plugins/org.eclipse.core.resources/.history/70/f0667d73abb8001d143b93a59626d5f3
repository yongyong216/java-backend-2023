package chapter3.B_VariableMethod;

class Triangle {
	double underside;
	double height;
	double diagonal;
	
	double area;
	double circumrerence;
}

public class Method2 {

	public static void main(String[] args) {
		Triangle triangle1 = new Triangle();
		triangle1.underside = 3;
		triangle1.height = 4;
		triangle1.diagonal = 5;
		
	}

}
