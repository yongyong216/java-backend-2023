package chapter3.D_Constructor;

class Circle {
	double radius;
	double diameter;
	double circumference;
	double area;
}

public class Constructor {

	public static void main(String[] args) {
		
		Circle circle1 = new Circle();  // Circle()의 괄호는 생성시 초기화 메서드이다.
		circle1.radius = 10;
		circle1.diameter = 20;
		circle1.circumference = 62.8;
		circle1.area = 314;
		
		double radius;
		
				
		
	}

}
