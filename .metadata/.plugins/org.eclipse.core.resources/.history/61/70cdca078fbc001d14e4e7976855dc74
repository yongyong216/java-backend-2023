package chapter4.D_Interface;

// 인터페이스
// 추상 클래스보다 추상화 정도가 높은 존재
// 추상 클래스와 다르게 일반 메서드와 일반 멤버 변수를 가질 수 없다. 

// 사용 방법:
// interface 인터페이스명 { }
interface ExampleInterface{
	// 변수를 사용할 때는 Public static final 형태로만 사용할 수 있음
	public static final int NUMBER = 10;  // 무조건 초기화 해줘야한다.
	
	// 메서드를 사용 할 때는 public abstract로 선언해야 한다.
	public abstract void method1();
	public void method2();
	void method3();
}

public class Interface {

	public static void main(String[] args) {
		

	}

}
